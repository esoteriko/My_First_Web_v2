<%= form_with scope: :user, url: users_path, local: true do |form| %>
    <div>
        <%= form.label :Nombre%><br>
        <%=form.text_field :name, placeholder: "nombre..." %>
    </div>
    <div>
         <%= form.label :Appeldio%><br>
        <%=form.text_field :last_name, placeholder: "apellido..." %>    
    </div>
    <div>
         <%= form.label :Grupo%><br>
        <%=form.select :group,%w[A1 A2 A3 A4 B1 B2 B3 B4], placeholder: "grupo" %>    
    </div>
    <div>
        <%=form.label :Numero_de_cuenta%><br>
        <%= form.text_field :count_number, placeholder: "numero de cuenta..."%>
    </div>
    <div>
    <%= form.label :Usuario %><br>
    <%= form.text_field :user_name, placeholder: "usario..."%>
    </div>

    <div><%= form.label :Contrase単a %><br>
    <%= form.password_field :password,  placeholder: "contrase単a..." %>
    </div>

    <div><%=form.label :Repite_la_contrase単a %><br>
        <%= form.password_field :password_2, placeholder: "repite la contrase単a..."%>
    </div>
    <br>
    <div>
    <%= form.submit "Aceptar" %>
    </div>
<% end %>